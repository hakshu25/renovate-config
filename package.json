{
  "name": "@flyhighair/renovate-config",
  "version": "0.0.1",
  "main": "none",
  "repository": "https://github.com/flyhighair/renovate-config.git",
  "author": "flyhighair <flyhighup.air25@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "renovate-config-validator"
  },
  "renovate-config": {
    "default": {
      "extends": [
        "config:base"
      ],
      "timezone": "Asia/Tokyo",
      "schedule": [
        "every weekend"
      ],
      "assignees": [
        "flyhighair",
        "hakshu"
      ],
      "labels": [
        "renovate"
      ],
      "patch": {
        "automerge": true
      },
      "minor": {
        "packageRules": [
          {
            "matchDepTypes": [
              "devDependencies"
            ],
            "automerge": true
          }
        ]
      },
      "node": {
        "supportPolicy": [
          "lts_latest"
        ]
      }
    }
  },
  "devDependencies": {
    "@commitlint/cli": "13.2.1",
    "@commitlint/config-conventional": "13.2.0",
    "husky": "7.0.4",
    "renovate": "28.8.4"
  }
}
